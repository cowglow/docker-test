{
  "name": "docker-test",
  "version": "1.0.0",
  "author": "@cowglow",
  "license": "MIT",
  "scripts": {
    "start": "docker-compose up",
    "stop": "docker-compose down",
    "dev": "gulp watch",
    "build": "gulp build",
    "test": "gulp archive && mocha --require @babel/register --reporter spec --timeout 5000"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/register": "^7.13.8",
    "apache-server-configs": "^4.1.0",
    "archiver": "^5.3.0",
    "del": "^6.0.0",
    "eslint": "^7.22.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-plugin-mocha": "^8.1.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-eslint": "^6.0.0",
    "gulp-header": "^2.0.9",
    "gulp-load-plugins": "^2.0.6",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "main.css": "2.1.0",
    "mocha": "^8.3.2",
    "modernizr": "3.11.4",
    "normalize.css": "8.0.1",
    "strip-json-comments": "^3.1.1"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "IE 11"
  ],
  "h5bp-configs": {
    "directories": {
      "archive": "archive",
      "dist": "htdocs",
      "src": "src",
      "test": "test"
    }
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE.txt",
    "package.json",
    "dist/",
    "../modernizr-config.json",
    "README.md"
  ],
  "dependencies": {}

}
